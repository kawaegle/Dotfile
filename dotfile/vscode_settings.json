{
    "material-icon-theme.showWelcomeMessage": false,
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "on",
    "epitech-c-cpp-headers.username": "kawaegle",
    "epitech-c-cpp-headers.login": "alecromski@epitech.eu",
    "epitech-c-cpp-headers.headerType": "post2017",
    "epitech-c-cpp-headers.usePragmaOnce": false,
    "epitech-c-cpp-headers.autoGenerateClasses": true,
    "epitech-c-cpp-headers.indentAccessSpecifiers": true,
    "files.defaultLanguage": "python3",
    "workbench.preferredDarkColorTheme": "Neofloss",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 4,
    "workbench.tree.renderIndentGuides": "always",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.codeLens": false,
    "editor.comments.ignoreEmptyLines": false,
    "editor.multiCursorMergeOverlapping": false,
    "editor.renderLineHighlight": "gutter",
    "editor.roundedSelection": false,
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "underline",
    "editor.find.autoFindInSelection": "always",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.quickSuggestionsDelay": 100,
    "editor.suggest.filterGraceful": false,
    "workbench.list.multiSelectModifier": "alt",
    "workbench.editor.untitled.labelFormat": "name",
    "window.title": "${activeEditorShort}",
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "none",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "C_Cpp.updateChannel": "Insiders",
    "editor.insertSpaces": false,
    "platformio-ide.disablePIOHomeStartup": true,
    "terminal.integrated.rendererType": "dom",
    "workbench.colorTheme": "Tokyo City",
    "terminal.integrated.sendKeybindingsToShell": true
}